<div class="container">
    <div class="col s12">
        <div class="card-panel green white-text center">Weekly Shifts for waiters</div>
    </div>

    {{#each all}}
    <ul class="collection with-header">
        <li class="collection-item"><h4>{{weekday}}</h4></li>
        {{#each waiters}}
        <li class="collection-item">{{this}}</li>
        {{/each}}
    </ul>
    {{/each}}

